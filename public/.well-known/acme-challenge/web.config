<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<staticContent>
			<clear />
			<mimeMap fileExtension="." mimeType="text/plain" />
		</staticContent>
		<rewrite>
			<rules>
				<clear />
			</rules>
			<outboundRules>
				<clear />
			</outboundRules>
		</rewrite>
		 <handlers>
			<clear />
			<add name="ACMEStaticFileHandler" path="*" verb="*" modules="StaticFileModule" resourceType="File" requireAccess="Read" />
        </handlers>
		<modules runAllManagedModulesForAllRequests="false">
			 <remove name="MaxMindModule" />
			 <remove name="Helicon.Ape" />
			 <remove name="ProjectHoneyPotModule" />
		</modules>
		<security>
			<ipSecurity allowUnlisted="true">
				<clear />
			</ipSecurity>
		</security>
	</system.webServer>
</configuration>